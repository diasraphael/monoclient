{
  "name": "monoclient",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:todo": "turbo build --filter=todo-*",
    "dev": "turbo run dev",
    "dev:todo": "turbo dev --filter=todo-web",
    "lint": "turbo run lint",
    "lint:fix": "turbo lint:fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "turbo test",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
