{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,ico}", "/css/*", "/_next/*", "/api/*"]
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "networking": {
    "allowedIpRanges": []
  }
}
